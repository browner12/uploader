{
    "name": "browner12/uploader",
    "type": "library",
    "description": "upload assets to web server",
    "keywords": [
        "browner12",
        "uploader"
    ],
    "homepage": "https://github.com/browner12/uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Brown",
            "email": "browner12@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0",
        "illuminate/support": "^6.0",
        "intervention/image": "~2",
        "symfony/http-foundation": "~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "browner12\\uploader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "browner12\\uploader\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "browner12\\uploader\\UploaderServiceProvider"
            ]
        }
    }
}
